{
  "name": "Spacer",
  "version": "0.9.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@egjs/flicking-plugins": "^4.7.0",
    "@egjs/react-flicking": "^4.10.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@turf/turf": "^6.5.0",
    "baseui": "^13.0.0",
    "create-cert": "^1.0.6",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.6",
    "i18next": "^23.1.0",
    "leaflet": "^1.9.4",
    "rc-texty": "^0.2.0",
    "react": "^18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-hook-geolocation": "^1.1.0",
    "react-i18next": "^13.0.0",
    "react-leaflet": "^4.2.1",
    "react-scripts": "5.0.1",
    "react-tilt": "^1.0.2",
    "react-transition-group": "^4.4.5",
    "reshake": "^2.0.0",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "^6.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build && npm run obfuscate-js && npm run minify-html-css",
    "deploy": "gh-pages -d build",
    "start": "PORT=3001 react-scripts start",
    "start:dev": "PORT=3001 REACT_APP_DEV_MODE=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "obfuscate-js": "javascript-obfuscator build/static/js --output build/static/js --config javascript-obfuscator-config.json",
    "minify-html-css": "html-minifier --input-dir build --output-dir build --file-ext html --minify-css true --minify-js false --remove-comments --collapse-whitespace --conservative-collapse"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "html-minifier": "^4.0.0",
    "javascript-obfuscator": "^4.1.1"
  }
}
